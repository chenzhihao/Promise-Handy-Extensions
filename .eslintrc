{
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "globals": {
    "jQuery": false,
    "_": false,
    "$": false,
    "console": false,
    "module": false,
    "require": false,
    "define": false,
    "__dirname": false
  }
}